{
    "prefixList": [
        {
            "alias": "sh",
            "url": "http://www.w3.org/ns/shacl#"
        },
        {
            "alias": "ex",
            "url": "http://w3id.org/gaia-x/ex#"
        }
    ],
    "shapes": [
        {
            "schema": "FlowerShape",
            "targetClassPrefix": "ex",
            "targetClassName": "Flowers",
            "constraints": [
                {
                    "path": {
                        "prefix": "ex",
                        "value": "color"
                    },
                    "in": [
                        {
                            "prefix": "ex",
                            "value": "Pink"
                        },
                        {
                            "prefix": "ex",
                            "value": "Purple"
                        }
                    ]
                }
            ]
        }
    ]
}
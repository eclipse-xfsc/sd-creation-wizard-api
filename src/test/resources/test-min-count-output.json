{
    "prefixList": [
        {
            "alias": "schema",
            "url": "http://schema.org/"
        },
        {
            "alias": "ex",
            "url": "http://w3id.org/gaia-x/ex#"
        },
        {
            "alias": "sh",
            "url": "http://www.w3.org/ns/shacl#"
        }
    ],
    "shapes": [
        {
            "schema": "TelephoneShape",
            "targetClassPrefix": "ex",
            "targetClassName": "phone",
            "constraints": [
                {
                    "path": {
                        "prefix": "ex",
                        "value": "countryCode"
                    },
                    "minCount": 1
                }
            ]
        }
    ]
}